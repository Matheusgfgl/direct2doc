{
  "name": "Direct2Doc",
  "private": true,
  "version": "1.0.0",
  "build": true,
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .ts,vue --ignore-path .gitignore .",
